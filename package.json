{
  "name": "territory-servant-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "start": "vue-cli-service serve --open"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.2",
    "@fortawesome/free-solid-svg-icons": "^5.2.0",
    "@fortawesome/vue-fontawesome": "^0.1.1",
    "@types/axios": "^0.14.0",
    "axios": "^0.18.0",
    "bootstrap": "^4.3.1",
    "bootstrap-vue": "^2.0.0-rc.16",
    "date-fns": "^1.29.0",
    "firebase": "^5.7.2",
    "firebaseui": "^3.5.1",
    "lodash": "^4.17.11",
    "vue": "^2.5.13",
    "vue-axios": "^2.1.2",
    "vue-router": "^3.0.1",
    "vuex": "^3.1.0",
    "vuex-cache": "^3.0.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.5.5",
    "@vue/cli-plugin-eslint": "^3.6.0",
    "@vue/cli-service": "^3.5.3",
    "@vue/eslint-config-airbnb": "^4.0.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.2.2",
    "vue-template-compiler": "^2.5.16"
  },
  "babel": {
    "presets": [
      "@vue/app"
    ]
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "plugin:vue/base",
      "@vue/airbnb"
    ],
    "env": {
      "node": true
    },
    "rules": {
      "max-len": [
        2,
        {
          "code": 125
        }
      ],
      "comma-dangle": [
        "error",
        "always-multiline"
      ],
      "import/prefer-default-export": "off",
      "no-await-in-loop": "off",
      "no-restricted-syntax": [
        "error",
        "ForInStatement",
        "LabeledStatement",
        "WithStatement"
      ],
      "no-console": [
        "error",
        {
          "allow": [
            "error"
          ]
        }
      ],
      "no-param-reassign": [
        "error",
        {
          "props": false
        }
      ],
      "no-return-assign": "off",
      "no-mixed-operators": "off",
      "import/no-cycle": "off",
      "import/extensions": "off",
      "import/order": "off",
      "no-alert": "off",
      "no-plusplus": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint",
      "sourceType": "module",
      "allowImportExportEverywhere": false,
      "codeFrame": false,
      "ecmaVersion": 2017
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
